<h3><%= title %></h3>
<% unless objs.empty? %>
  <ul class="no-bullet">
    <% objs.each do |obj| %>
      <li>
        <h5>
          <%= obj_title(obj) %>
          <% ajax_remote = (obj.class.name.demodulize != 'Experience' ? true : false) %>
          <%= link_to "<i class=\"fi-pencil right\"></i>".html_safe, [:edit, @resume, obj], title: 'Edit', remote: ajax_remote %>
          <%= link_to "<i class=\"fi-trash right\"></i>".html_safe, [@resume, obj], method: :delete, data: { confirm: 'Are you sure?' }, title: 'Delete', class: 'warning' %>
        </h5>
      </li>
      <hr />
    <% end %>
  </ul>
<% end %>
